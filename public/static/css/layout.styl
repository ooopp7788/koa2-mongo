//遮罩层
#shade-layer
  display none
  position fixed
  top 0
  z-index 6666
  background rgba(0,0,0,0.3)
  width 100%
  height 100%
  cursor pointer
//弹出层
.pop
  position absolute
  width 400px
  z-index 9999
  margin 0
  padding 0
  border-radius 3px
  background #ffffff
  .win-hint-ensure
    display block
    width 100%
    margin-top 20px

//登陆
#pop-login
  border 1px solid #ffffff
  .login-logo
    width 180px
    height 150px
    margin 0 auto
    margin-top -108px
  .login-tool
    height 24px
    width auto
    position absolute
    right 0
    top 0
    .close
      height @height
      width @height
      cursor pointer
      &:hover
        color $colorRed
    .help
      height @height
      width @height
  .form-login
    width 340px
    margin 0 auto
    .form-info
      border 1px solid #bfbfbf
      box-sizing border-box
      div
        margin 0 8px
        &.area1
          border-bottom 1px solid #bfbfbf
      input
        width 322px
        height 50px
        border none
        font-size 16px
        font-family serif
    .area-tool
      width 340px
      height 40px
      line-height 40px
      a
        color #3886f2
    #area-login-btn
      width 100%
      height 52px
      line-height 52px
      text-align center
      background #67bdce
      border none
      font-family serif
      cursor pointer
      a
        width @width
        height @height
        color #ffffff
        font-size 20px
  #area-login-tool
    width 340px
    height 20px
    line-height 20px
    margin 20px auto
    img
      margin-left 8px
      margin-top -3px
  top 300px
#pop-confirm
  height 100px
  text-align center
  button
    color #ffffff
    cursor pointer
    margin 20px
    width 80px
    height 26px
    border none
    border-radius 3px
    &#btn_cancle_ensure
      background #3a9bd9
      &:hover
        background darken(@background,20%)
    &#btn_ok_ensure
      background #e74c3c
      &:hover
        background darken(@background,20%)
#pop-follow
  height 150px
  padding 20px 30px
  #win-btn-close
    position absolute
    right 20px
    top 8px
    cursor pointer
    &:hover
      color #e74c3c
  .win-hint-ensure
    margin 0
    font-size large
  .unit
    margin-top 14px
    border-bottom 1px dashed
    margin-bottom 10px
    height 80px
    select
      margin 0 10px
      min-width 140px
      outline none
      height 24px
      line-height @height
      border-radius 3px
      border 1px solid #bbbbbb
      background #ffffff
      box-shadow 0 1px 1px rgba(0,0,0,0.1)
      cursor pointer

    p
      &.p1
        width 60px
        height 24px
        line-height @height
        text-align center
        background #EEEEEE
        border-radius 3px
      &.hint
        margin 8px 10px
        color #999
  .unit-tool
    width 80px
    #btn-do-follow
      background #95ba12
      border 1px solid @background
      border-radius 3px
      height 28px
      color #ffffff
      padding 0 8px
      cursor pointer
      &:hover
        background darken(@background,20%)
