c1 = #eb4556
.area-calendar
  display none
  width 358px
  border 1px solid #949494
  box-shadow 1px 1px 2px rgba(#000, 0.1)
  background white
  font-size 13px
  color #2F302B
  border-radius 6px
  padding 12px
  position absolute
  top 76%
  right 5%
  z-index 1

.calendar-mod
  width 360px
  background white
  border-bottom 1px solid #E2E2E2
  padding-bottom 6px
  span
    float left
    width 372px
    height 24px
    line-height 16px
    font-size 14px
  .close
    width 27px
    height 15px
    cursor pointer
    float right
    margin-top -4px

#calendar-start-date, #calendar-end-date
  width 86px
  height 23px
  border 1px solid #D9D9D9
  margin 10px 0 10px 0
  padding 0 6px
  &:hover, &:focus
    box-shadow 0 0 6px rgba(#aeaeae, 0.5) inset

.year-mouth
  margin 10px 0 8px 0
  font-size 14px

.unit-year
  margin 0 0 12px 0
  cursor pointer
  overflow hidden
  width 225px
  .btn-year
    float left
    width 73px
    height 26px
    line-height 26px
    text-align center
    border 1px solid #FFC6CB
    margin 0 -1px 0 0
    &:hover
      color #fff
      background-color rgba(251, 91, 104, 0.9)
  .btn-year.thisYear
    color #FD5462
    background rgba(253, 76, 91, 0.1)

.unit-mouth
  margin 0 0 16px 0
  cursor pointer
  width 354px
  .btn-mouth
    float left
    width 58px
    height 27px
    line-height 27px
    text-align center
    border 1px solid #FFC6CB
    margin -1px -1px 0px 0px
    background #f3f3f3
    &.disable
      color #999
      cursor default
      &:hover
        color #999
        background #f3f3f3
    &.active
      background none
    &:hover
      color #fff
      background-color rgba(251, 91, 104, 0.9)
  .btn-mouth.thisMouth
    color #FD5462
    background rgba(253, 76, 91, 0.1)

#calendar-btn
  width 112px
  float right
  margin 10px 0 0 0

.calendar-sure
  width 76px
  height 22px
  line-height 22px
  text-align center
  border 1px solid #EBEBEB
  border-radius 4px
  cursor pointer
  color #fff
  background rgba(251, 91, 104, 0.9)
  &:hover
    background-color #eb4556
    border 1px solid #FD4C5B
  &.active
    color #fff
    background-color #FD4C5B
    border 1px solid #399ad9

.prompt
  float left
  color #333
  font-size 14px
  margin 0 0 3px 0
  .asterisk
    color red

//datapicker
.ui-datepicker
  width 20em
  padding .2em .2em 0
  display none
  border 1px solid #ddd
  box-shadow 1px 1px 2px rgba(0, 0, 0, 0.1)
  background-color #fff
  .ui-datepicker-header
    position relative
    padding .2em 0
    background-color lighten(c1, 20%)
    color #fff
    .ui-icon
      height 16px
      overflow hidden
      line-height 50px
    .ui-icon-circle-triangle-w
      background-position -240px -96px
    .ui-icon-circle-triangle-e
      background-position -264px -96px
  .ui-datepicker-prev
    left 2px
  .ui-datepicker-next
    right 2px
  .ui-datepicker-prev-hover
    left 1px
  .ui-datepicker-next-hover
    right 1px
  .ui-datepicker-title
    margin 0 2.3em
    line-height 1.8em
    text-align center
    select
      font-size 1em
      margin 1px 0
      height auto
      min-width 10px
      border-radius 0
      box-shadow none
  table
    width 100%
    font-size .9em
    border-collapse collapse
    margin 0 0 .4em
  th
    padding .5em .3em
    text-align center
    font-weight bold
    border 0
    color darken(c1, 30%)
  td
    border 0
    padding 1px
  .ui-datepicker-buttonpane
    background-image none
    margin .7em 0 0 0
    padding 0 .2em
    border-left 0
    border-right 0
    border-bottom 0
    button
      float right
      margin .5em .2em .4em
      cursor pointer
      padding .2em .6em .3em .6em
      width auto
      overflow visible
      &.ui-datepicker-current
        float left
  &.ui-datepicker-multi
    width auto

.ui-datepicker .ui-datepicker-prev,
.ui-datepicker .ui-datepicker-next
  position absolute
  top 2px
  width 1.8em
  height 1.8em

.ui-datepicker .ui-datepicker-prev-hover,
.ui-datepicker .ui-datepicker-next-hover
  top 1px

.ui-datepicker .ui-datepicker-prev span,
.ui-datepicker .ui-datepicker-next span
  color white
  display block
  position absolute
  left 50%
  margin-left -8px
  top 50%
  margin-top -12px

.ui-datepicker select.ui-datepicker-month
  margin 0px 0 2px 8px
  width 36%

.ui-datepicker select.ui-datepicker-year
  width 35%
  margin 0 8px 2px 0

.ui-datepicker td span,
.ui-datepicker td a
  display block
  color #333
  background-color lighten(c1, 80%)
  padding .2em
  text-align right
  text-decoration none

.ui-datepicker
  td
    .ui-state-hover
      background-color #ddd
  .ui-datepicker-today
    a, span
      color #fff
      background-color #ddd
  .ui-datepicker-current-day
    a, span
      color #fff
      background-color lighten(#eb4556, 10%)
  .ui-state-disabled
    .ui-state-default
      color #999
      background #fcfcfc

.ui-datepicker-multi
  .ui-datepicker-group
    float left
    table
      width 95%
      margin 0 auto .4em
  .ui-datepicker-buttonpane
    clear left

.ui-datepicker-multi-2
  .ui-datepicker-group
    width 50%

.ui-datepicker-multi-3
  .ui-datepicker-group
    width 33.3%

.ui-datepicker-multi-4
  .ui-datepicker-group
    width 25%

.ui-datepicker-multi .ui-datepicker-group-last .ui-datepicker-header,
.ui-datepicker-multi .ui-datepicker-group-middle .ui-datepicker-header
  border-left-width 0

.ui-datepicker-row-break
  clear both
  width 100%
  font-size 0

.ui-datepicker-rtl
  direction rtl
  .ui-datepicker-prev
    right 2px
    left auto
    &:hover
      right 1px
      left auto
  .ui-datepicker-next
    left 2px
    right auto
    &:hover
      left 1px
      right auto
  .ui-datepicker-buttonpane
    clear right
    button
      float left

.ui-datepicker-rtl .ui-datepicker-buttonpane button.ui-datepicker-current,
.ui-datepicker-rtl .ui-datepicker-group
  float right

.ui-datepicker-rtl .ui-datepicker-group-last .ui-datepicker-header,
.ui-datepicker-rtl .ui-datepicker-group-middle .ui-datepicker-header
  border-right-width 0
  border-left-width 1px

.ui-datepicker-calendar
  width 233px
  height 143px

#ui-datepicker-div
  z-index 9 !important